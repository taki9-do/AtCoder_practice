#include <iostream>
#include<math.h>
#include<vector>
#include <algorithm>
#include <map>  
#include <string>
using namespace std;
#define ll long long
int main(void){
    // Your code here!
    ll N;
    cin>>N;
    ll p[N],q[N];
 
    for(int i=0;i<N;i++){
        cin>>p[i];
 
        //int a;
        //cin>>a;
        //p.push_back(a);
    }
    
    for(int i=0;i<N;i++){
        //q.at(p[i])=i;
        q[p[i]-1]=i+1;
    }
    
    for(int i=0;i<N;i++){
        cout<<q[i]<<" ";
    }
}
