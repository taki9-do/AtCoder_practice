#include <bits/stdc++.h>
using namespace std;
#define rep(i,n) for (int i = 0; i < (n); ++i)
using ll = long long;
int main(void){
    // Your code here!
    string S;
    int K;
    cin>>S>>K;
    
    sort(S.begin(),S.end());
    
    rep(i,K-1) next_permutation(S.begin(),S.end());
    cout<<S;
}
