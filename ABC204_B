#include <iostream>
#include<math.h>
#include<vector>
#include <algorithm>
#include <map>  
using namespace std;
#define ll long long

int main(void){
    // Your code here!
   int N,sum=0;
   cin>>N;
   vector<int> A;
   
   for(int i=0;i<N;i++){
       int x;
       cin>>x;
       A.push_back(x);
   }
   
   for(int i=0;i<N;i++){
       if(A[i]<=10) continue;
       else if(A[i]>10)  sum+=A[i]-10;
   }

   cout<<sum<<endl;
   
   
}
