#include <bits/stdc++.h>
using namespace std;
#define rep(i,n) for (int i = 0; i < (n); ++i)
using ll = long long;
int main(void){
    // Your code here!
    ll N,P;
    cin>>N>>P;
    vector<ll> A(N);
    rep(i,N) cin>>A[i];
    
    ll count=0;
    
    rep(i,N){
        if(A[i]<P) count++;
    }
    

    cout<<count;
}
