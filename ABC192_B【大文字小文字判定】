#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main(void){
    // Your code here!
    string S;
    cin>>S;
    bool flag=true;
    
    for(int i=1;i<=S.size();i++){
        if(i%2==1&&islower(S[i-1])==false) flag=false;
        else if(i%2==0&&isupper(S[i-1])==false) flag=false;
    }
    if(flag==true) cout<<"Yes";
    else cout<<"No";
    
    
    
    
}
