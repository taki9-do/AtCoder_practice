#include <bits/stdc++.h>
using namespace std;
#define rep(i,n) for (int i = 0; i < (n); ++i)
using ll = long long;

int main(void){
    // Your code here!
    int N;
    cin>>N;
    vector<ll> X(N),Y(N);
    rep(i,N) cin >> X[i]>>Y[i];
    
    int count=0;
    
    ll x1,x2,y1,y2;
    for(int i=0;i<N-2;i++){
        for(int j=i+1;j<N-1;j++){
            for(int k=j+1;k<N;k++){
                x1=X[j]-X[i];
                x2=X[k]-X[i];
                y1=Y[j]-Y[i];
                y2=Y[k]-Y[i];
                //０除算を避けるため
                if(x2*y1==x1*y2) {
                    cout<<"Yes"; 
                    return 0;
                }
            }
        }
    }
    
    cout<<"No";
    
}
