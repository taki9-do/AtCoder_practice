#include <bits/stdc++.h>
#define rep(i,n) for (int i = 0; i < (n); ++i)
using namespace std;
using ll = long long;
int main(void){
    // Your code here!
    ll A;double B;cin>>A>>B;
    
    //double → intへの暗黙的な型変換において、
    //値を壊れないようにするため
    //9.9999999....　だと、998になったりする。
    ll llB=B*100+0.5;
    ll C=A*llB;
    C/=100;
    
    cout<<C;
}
