#include <bits/stdc++.h>
using namespace std;
#define rep(i,n) for (int i = 0; i < (n); ++i)
using ll = long long;
int main(void){
    // Your code here!
    int N;cin>>N;
    vector<ll> H(N);
    rep(i,N) cin>>H[i];
    rep(i,N){
        if(H[i]>=H[i+1]){
            cout<<H[i];
            return 0;
        }
        if(i==N-1){
            cout<<H[i];
            return 0;
        }
    }
}
