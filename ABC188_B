#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main(void){
    // Your code here!
    ll N,x=0;
    cin>>N;
    vector<int> A(N),B(N);
    for(int i=0;i<N;i++) cin>>A[i];
    for(int i=0;i<N;i++) cin>>B[i];
    for(int i=0;i<N;i++){
        x+=(A[i]*B[i]);
    }
    
    if(x==0) cout<<"Yes";
    else cout<<"No";
}
