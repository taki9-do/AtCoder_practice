#include <iostream>
#include<math.h>
#include<vector>
#include <algorithm>
#include <map>  
using namespace std;
#define ll long long
int main(void){
    // Your code here!
    int N,K,sum=0;
    cin>>N>>K;
    
    for(int i=0;i<N;i++){
        for(int j=0;j<K;j++){
            sum+=(i+1)*100+j+1;
        }
    }
    
    cout<<sum<<endl;
}
