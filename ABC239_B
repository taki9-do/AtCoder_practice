#include <bits/stdc++.h>
#define rep(i,n) for (int i = 0; i < (n); ++i)
using namespace std;
using ll = long long;
int main(void){
    // Your code here!
    ll X;cin>>X;
    double ans=X/10;
    if(X>=0) cout<<X/10;
    else{
        if(X>-10) cout<<-1;
        else if(X%10==0) cout<<X/10;
        else cout<<(X/10)-1;
    } 
    
}
