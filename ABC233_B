#include <bits/stdc++.h>
using namespace std;
#define rep(i,n) for (int i = 0; i < (n); ++i)
using ll = long long;
int main(void){
    // Your code here!
    int L,R;cin>>L>>R;
    string S;cin>>S;
    rep(i,L-1) cout<<S[i];
    for(int i=R-1;i>=L-1;i--) cout<<S[i];
    for(int i=R;i<S.size();i++) cout<<S[i];
    
}
