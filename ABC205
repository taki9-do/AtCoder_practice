#include <bits/stdc++.h>
using namespace std;
#define rep(i,n) for (int i = 0; i < (n); ++i)
using ll = long long;


int main(void){
    // Your code here!
    ll A,B,C,ans1,ans2;
    cin>>A>>B>>C;
    
    if(C%2==0){
        if(abs(A)>abs(B)) cout<<">";
        else if(abs(A)<abs(B)) cout<<"<";
        else if(abs(A)==abs(B)) cout<<"=";
    }
    else if(C%2==1){
        if(A>B) cout<<">";
        else if(A<B) cout<<"<";
        else if(A==B) cout<<"=";
    }
    
    
    
}
