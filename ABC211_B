#include <iostream>
#include<math.h>
#include<vector>
#include <algorithm>
#include <map>  

using namespace std;

#define ll long long
int main(void){
    // Your code here!
   map<string,int> mp;
   mp["H"]=0;
   mp["2B"]=0;
   mp["3B"]=0;
   mp["HR"]=0;
   
   string S[4];
   for(int i=0;i<(int)mp.size();i++){
       cin>>S[i];
       mp[S[i]]++;
   }
   
   if(mp["H"]>0&&mp["2B"]>0&&mp["3B"]>0&&mp["HR"]>0) cout<<"Yes"<<endl;
   else cout<<"No"<<endl;
   

    //if(step==true||same==true) cout<<"Weak"<<endl;
    //else cout<<"Strong"<<endl;


}
