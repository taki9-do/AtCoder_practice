#include <bits/stdc++.h>
#define rep(i,n) for (int i = 0; i < (n); ++i)
using namespace std;
using ll = long long;
int main(void){
    // Your code here!
    ll N;cin>>N;
    vector<ll> A(N);
    map<int,ll> mp;
    rep(i,N) cin>>A[i],mp[A[i]]++;
    
    ll ans=0;
    ans+=mp[100]*mp[400];
    ans+=mp[200]*mp[300];
    
    cout<<ans<<endl;
}
