#include <bits/stdc++.h>
using namespace std;
#define rep(i,n) for (int i = 0; i < (n); ++i)
using ll = long long;
int main(void){
    // Your code here!
    ll N,D;cin>>N>>D;
    ll cnt=0;
    rep(i,N){
        ll x,y;cin>>x>>y;
        double dis=sqrt(x*x+y*y);
        if(dis<=D) cnt++;
    }
    
    cout<<cnt;
}
